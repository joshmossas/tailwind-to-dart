{
    "name": "tailwind-to-dart",
    "version": "0.0.3",
    "description": "Convert your tailwind config to dart code for your flutter apps",
    "type": "module",
    "main": "dist/main.mjs",
    "scripts": {
        "build": "unbuild",
        "lint": "eslint ./src/**/*.ts",
        "format": "prettier --write ./src",
        "test": "vitest run",
        "test:watch": "vitest",
        "generate-example": "./dist/cli.mjs --config ./example/tailwind.config.js --output ./example/tailwind.dart --format"
    },
    "keywords": [
        "tailwind",
        "tailwindcss",
        "flutter",
        "dart"
    ],
    "author": "Joshua Sosso",
    "files": [
        "dist",
        "src"
    ],
    "bin": {
        "tailwind-to-dart": "./dist/cli.mjs"
    },
    "license": "ISC",
    "dependencies": {
        "c12": "^1.7.0",
        "citty": "^0.1.5",
        "consola": "^3.2.3",
        "pathe": "^1.1.2",
        "scule": "^1.3.0",
        "tailwindcss": "^3.4.1"
    },
    "devDependencies": {
        "@types/node": "^20.11.17",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-promise": "^6.0.0",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3",
        "unbuild": "^2.0.0",
        "vite": "^5.1.0",
        "vitest": "^1.2.2"
    }
}
